<h1>Listing quote_categories</h1>

<table>
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quote_categories.each do |quote_category| %>
  <tr>
    <td><%= quote_category.title %></td>
    <td><%= link_to 'Show', quote_category %></td>
    <td><%= link_to 'Edit', edit_quote_category_path(quote_category) %></td>
    <td><%= link_to 'Destroy', quote_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Quote category', new_quote_category_path %>
